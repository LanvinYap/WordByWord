{
  "expo": {
    "name": "WordByWord",
    "slug": "WordByWord",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./components/images/wordbyword-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./components/images/wordbyword-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ebd9c3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
